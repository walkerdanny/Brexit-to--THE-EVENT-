{
	"manifest_version": 2,

	"name": "Brexit to [THE EVENT]",
    "short_name": "[THE EVENT]",
	"description": "Replaces Brexit with [THE EVENT], an equally catastrophic and unmentionable disaster.",
	"author": "Danny Walker",
	"version": "0.1",

	"icons": {
		"16": "icon-16.png",
		"32": "icon-32.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}
